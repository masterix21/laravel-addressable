{
    "name": "masterix21/laravel-addressable",
    "description": "Addresses for any Eloquent model",
    "keywords": [
        "masterix21",
        "laravel-addressable"
    ],
    "homepage": "https://github.com/spatie/laravel-addressable",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Longo",
            "email": "l.longo@ambita.it",
            "homepage": "https://masterix.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Masterix21\\Addressable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masterix21\\Addressable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AddressableServiceProvider"
            ],
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
